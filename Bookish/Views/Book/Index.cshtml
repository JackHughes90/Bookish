@model List<Book>;

@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Books</h1>
    <ul>
        @foreach (Book book in Model)
        {
            <li>
                <h2>@book.Title</h2>
                @* <p>By @book.Author</p> *@
            </li>
        }
    </ul>
    
</div>
